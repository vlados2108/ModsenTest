generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("CONNECTION_STRING")
}

model User {
  id                 Int     @id @default(autoincrement())
  name               String?
  password           String?
  hashedRefreshToken String?
}

model Meetup {
  id          Int       @id(map: "meetup_pkey") @default(autoincrement())
  name        String?
  description String?
  tegs        String?
  time        DateTime? @db.Timestamp(6)
  place       String?
}
