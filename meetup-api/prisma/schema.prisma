generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Meetup {
  id          Int      @id @default(autoincrement())
  name        String   @default("")
  description String   @default("")
  tegs        String   @default("")
  time        DateTime @default(now()) @db.Timestamp(6)
  place       String   @default("")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String @default("")
  password String @default("")
}
